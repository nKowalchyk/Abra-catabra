if(timeToHit &gt; 0){
    timeToHit--;
}
else{
    timeToHit = 0;
}

if(hp &lt;= 0){
   game_restart();
}

if((place_meeting(x, y, obj_BasicPig) || place_meeting(x, y, obj_FirePig)) &amp;&amp; timeToHit == 0){
