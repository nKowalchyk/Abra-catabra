            <string>///Projectiles

shootRight = keyboard_check(vk_right);
shootLeft = keyboard_check(vk_left);

if(stepCounter &gt; 0)
{
    stepCounter--;
}
else
{
    stepCounter = 20;
    canFire = true;
}

if(shootLeft &amp;&amp; canFire)
{
    canFire = false;
    
    if(charge == 0)
    {
        charge = 1;
    }
    else if(charge &gt; 0)
    {
        charge++;
        if(charge &gt; 3)
        {
            charge = 3;
        }
    }
}
else if(keyboard_check_released(vk_left) &amp;&amp; charge == 3)
{
    bullet = instance_create(x, y, obj_Bullet)
    bullet.damage = charge;
    bullet.speed = 15;
    bullet.image_xscale *= 1.5;
    bullet.image_yscale *= 1.5;
    if(sign(image_xscale == 1)) bullet.direction = 0;
    if(sign(image_xscale == -1)) bullet.direction = 180;
    charge = 0;
}
else if(keyboard_check_pressed(vk_right) &amp;&amp; canFire)
{
    canFire = false;
    
    bullet = instance_create(x, y, obj_Bullet);
    bullet.speed = 15;
    if(sign(image_xscale == 1)) bullet.direction = 0;
    if(sign(image_xscale == -1)) bullet.direction = 180;
}
