          <argument>
            <kind>1</kind>
            <string>///Projectiles

shootRight = keyboard_check(vk_right);
shootLeft = keyboard_check(vk_up);

if(canFire){
    isFiring = false;
}

if(stepCounter &gt; 0)
{
    stepCounter--;
}
else
{
    stepCounter = 10;
    canFire = true;
}

if(shootLeft &amp;&amp; obj_Familiar.charge &gt;= 3 &amp;&amp; canFire){
    canFire = false;
    isCharging = true;
    if(stepCounter % 10 == 0 &amp;&amp; charge &lt; 3){
        charge++;
        audio_play_sound(SFX_Charge, 1, false);
        if(obj_Familiar.charge &gt; 0){
            obj_Familiar.charge--;
        }
    }
    else if(charge &gt; 3){
        charge = 3;
    }
}
else if(keyboard_check_released(vk_up) &amp;&amp; charge &lt; 3){
    isCharging = false;
    charge = 0;
}
else if(keyboard_check_released(vk_up) &amp;&amp; charge &gt;= 3){   
    isCharging = false;
    isFiring = true;
    audio_play_sound(SFX_LargeProjectile, 1, false);
    bullet = instance_create(x, y - 25, obj_LargeBullet)
    bullet.damage = charge;
    bullet.speed = 25;
    if(sign(image_xscale == 1)) bullet.direction = 0;
    if(sign(image_xscale == -1)) bullet.direction = 180;
    bullet.image_xscale *= sign(image_xscale);
    charge = 0;
}
else if(shootRight &amp;&amp; canFire){   
    canFire = false;
    isFiring = true;
    stepCounter = 10;
    audio_play_sound(SFX_Projectile, 1, false);
    bullet = instance_create(x, y - 25, obj_Bullet);
    bullet.speed = 25;
    if(sign(image_xscale == 1)) bullet.direction = 0;
    if(sign(image_xscale == -1)) bullet.direction = 180;
}
