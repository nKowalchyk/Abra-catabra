            <string>///Patrol Code
//collision code

xSpeed = dir * moveSpeed;
ySpeed += grav;

if(stepCounter == 0){
    stepCounter = 60;
    canFire = true;
}
else{
    stepCounter--;
}

if(place_meeting(x + xSpeed, y - pixelTolerance, obj_Platform)){
    while(!place_meeting(x + sign(xSpeed), y, obj_Platform)){
        x += sign(xSpeed);
    }
    xSpeed = 0;
    dir *= -1;
}

if(fearOfHeights)
{
    if(place_meeting(x+xSpeed,y - pixelTolerance - 8, obj_invisPlatform))
    {
        while(!place_meeting(x + sign(xSpeed), y, obj_invisPlatform))
        {
            x += sign(xSpeed);
        }
    xSpeed = 0;
    
    dir *= -1;
    }
}

x += xSpeed;

if(place_meeting(x, y + ySpeed, obj_Platform)){
    while(!place_meeting(x, y + sign(ySpeed), obj_Platform)){
        y += sign(ySpeed);
    }
    ySpeed = 0;
}

y += ySpeed;
